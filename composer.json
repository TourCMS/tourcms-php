{
    "name": "tourcms/tourcms-php",
    "type": "library",
    "description": "TourCMS API wrapper for PHP",
    "keywords": ["tourcms","api"],
    "homepage": "https://github.com/TourCMS/tourcms-php",
    "license": "MIT",
    "authors": [
        {
            "name": "TourCMS",
            "email": "support@tourcms.com",
            "homepage": "http://www.tourcms.com"
        }
    ],
    "scripts": {
        "test": "phpunit",
        "test:watch": [
            "Composer\\Config::disableProcessTimeout",
            "phpunit-watcher watch"
        ]
    },
    "require": {
        "php": ">=7.1",
        "psr/simple-cache": "^1.0",
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {"TourCMS\\Utils\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
            "branch-alias": {
                "dev-master": "3.2.x-dev",
                "dev-v1.x": "1.9.x-dev"
            }
        },
    "require-dev": {
        "spatie/phpunit-watcher": "^1.22",
        "matthiasmullie/scrapbook": "^1.4",
        "league/flysystem": "^1.0"
    }
}
